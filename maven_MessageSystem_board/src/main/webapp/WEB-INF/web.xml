<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
     <servlet-name>JoinService</servlet-name>
     <display-name>JoinService</display-name>
     <description></description>
     <servlet-class>com.smhrd.controller.JoinService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteService</servlet-name>
  	<display-name>DeleteService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.DeleteService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BoardService</servlet-name>
  	<display-name>BoardService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.BoardService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DelBoardService</servlet-name>
  	<display-name>DelBoardService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.DelBoardService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>IdCheckService</servlet-name>
  	<display-name>IdCheckService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.IdCheckService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FrontController</servlet-name>
  	<display-name>FrontController</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.frontcontroller.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>JoinService</servlet-name>
     <url-pattern>/JoinService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteService</servlet-name>
  	<url-pattern>/DeleteService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BoardService</servlet-name>
  	<url-pattern>/BoardService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DelBoardService</servlet-name>
  	<url-pattern>/DelBoardService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>IdCheckService</servlet-name>
  	<url-pattern>/IdCheckService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>